{
  "name": "easywsdl/tophp",
  "description": "Library for generating PHP classes from wsdl.",
  "type": "library",
  "license": "GPL-3.0",
  "require": {
    "php": ">=7.1",
    "nette/php-generator": "~v3.2.3",
    "ext-soap": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "nette/tester": "^2.0",
    "nette/bootstrap": "~2.4@dev",
    "nette/robot-loader": "~2.4@dev",
    "tracy/tracy": "^2.5",
    "ninjify/nunjuck": "^0.2.0"
  },
  "authors": [
    {
    "name": "Jiří Zima",
    "email": "info@thestormwinter.dev"
    }
  ],
  "autoload": {
    "psr-4": {
      "EasyWsdl\\ToPHP\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EasyWsdl\\ToPHP\\Tests\\": "tests"
    }
  },
  "config": {
    "platform": {
      "php": "7.1"
    }
  }
}
